{
  "name": "healthcareDapp",
  "version": "1.0.0",
  "description": "A decentralized healthcare DApp",
  "main": "truffle-config.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile": "truffle compile",
    "migrate": "truffle migrate --reset",
    "deploy": "npm run compile && npm run migrate",
    "ganache": "ganache-cli --port 7545 --deterministic",
    "dev": "npm run ganache & npm start",
    "copy-abi": "node scripts/copyABI.js",
    "setup": "npm install && npm run deploy && npm run copy-abi",
    "test-contracts": "truffle test",
    "api": "cd api && python app.py",
    "install-python": "pip install -r requirements.txt",
    "install-api": "cd api && pip install -r requirements.txt",
    "setup-full": "npm install && npm run install-python && npm run deploy && npm run copy-abi",
    "dev-full": "npm run ganache & npm run api & npm start"
  },
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.1.1",
    "bootstrap": "^5.3.7",
    "ethers": "^6.14.3",
    "fs-extra": "^11.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "react-scripts": "^5.0.1",
    "web-vitals": "^5.0.2"
  },
  "devDependencies": {
    "truffle": "^5.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
